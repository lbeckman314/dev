AuthPass	server/ssmtp.conf	/^AuthPass=PASSWORD$/;"	k
AuthUser	server/ssmtp.conf	/^AuthUser=USERNAME@gmail.com$/;"	k
UseSTARTTLS	server/ssmtp.conf	/^UseSTARTTLS=YES$/;"	k
UseTLS	server/ssmtp.conf	/^UseTLS=YES$/;"	k
\x20https	server/cgit.conf	/^RewriteRule ^ https:\/\/%{SERVER_NAME}%{REQUEST_URI} [END,QSA,R=permanent]$/;"	k
\x20https	server/git.conf	/^RewriteRule ^ https:\/\/%{SERVER_NAME}%{REQUEST_URI} [END,QSA,R=permanent]$/;"	k
hostname	server/ssmtp.conf	/^hostname=IP_ADDRESS$/;"	k
if	server/ddclient.conf	/^if=em1                 # use certain interface (ifconfig -a, to get the current IP)$/;"	k
login	server/ddclient.conf	/^login=WEBSITE.com      # the name of the A\/AAAA record$/;"	k
mailhub	server/ssmtp.conf	/^mailhub=smtp.gmail.com:587$/;"	k
password	server/ddclient.conf	/^password=ABCDEFGH      # the key of the A\/AAAA record$/;"	k
protocol	server/ddclient.conf	/^protocol=dyndns2       # default$/;"	k
root	server/ssmtp.conf	/^root=USERNAME@gmail.com$/;"	k
server	server/ddclient.conf	/^server=dyn.dns.he.net  # default$/;"	k
use	server/ddclient.conf	/^use=if                 # use the eithernet card (to get the current IP)$/;"	k
